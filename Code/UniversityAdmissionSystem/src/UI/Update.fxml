<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DAL.RegistrationsController">
    <TableView fx:id="registrationsTable" prefWidth="600" prefHeight="400">
        <columns>
            <TableColumn text="First Name" fx:id="firstNameColumn" />
            <TableColumn text="Last Name" fx:id="lastNameColumn" />
            <TableColumn text="Email" fx:id="emailColumn" />
            <TableColumn text="Phone Number" fx:id="phoneNumberColumn" />
            <TableColumn text="Address" fx:id="addressColumn" />
            <TableColumn text="Date of Birth" fx:id="dateOfBirthColumn" />
            <TableColumn text="Password" fx:id="passwordColumn" />
        </columns>
    </TableView>
</AnchorPane>
